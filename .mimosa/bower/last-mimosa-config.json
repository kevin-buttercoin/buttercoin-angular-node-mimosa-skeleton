{
  "watch": true,
  "bowerDir": {
    "path": ".mimosa/bower/bower_components",
    "clean": true,
    "pathFull": ""
  },
  "copy": {
    "enabled": true,
    "trackChanges": true,
    "outRoot": "",
    "defaultStrategy": "vendorRoot",
    "strategy": {
      "*": "vendorRoot"
    },
    "forceLatest": true,
    "exclude": [],
    "overridesArrays": {
      "chosen": [
        "chosen.jquery.js",
        "chosen.css",
